{
    "database": {
      "rules": "database.rules.json"
    },
    "firestore": {
      "rules": "firestore.rules",
      "indexes": "firestore.indexes.json"
    },
    "functions": {
      "predeploy": [
      ]
    },
    "hosting": {
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [{
        "source": "/feed",
        "destination": "/feed.html"
      }]
    },
    "storage": {
      "rules": "storage.rules"
    }
  }
  